<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="1483147381-0" author="app_core">
        <createSequence sequenceName="seq_session_interview" startValue="1050" incrementBy="50" />
    </changeSet>
    <changeSet id="1483147381-1" author="app_core">
        <createTable tableName="session_interview">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="code" type="nvarchar(250)">
                <constraints nullable="false" />
            </column>
            <column name="date_debut" type="date">
                <constraints nullable="true" />
            </column>
            <column name="date_fin" type="date">
                <constraints nullable="true" />
            </column>
            <column name="heure_debut" type="date">
                <constraints nullable="true" />
            </column>
            <column name="heure_fin" type="date">
                <constraints nullable="true" />
            </column>
            <column name="questionnaire_id" type="bigint">
                <constraints nullable="true" />
            </column>
        </createTable>
    </changeSet>

    <changeSet id="1483147381-data" author="app_core">
        <loadData
            file="liquibase/data/sessionInterview.csv"
            separator=";"
            tableName="session_interview"
            usePreparedStatements="true">
            <column name="id" type="numeric" />
            <column name="code" type="nvarchar(250)" />
            <column name="date_debut" type="date" />
            <column name="date_fin" type="date" />
            <column name="heure_debut" type="date" />
            <column name="heure_fin" type="date" />
        </loadData>
    </changeSet>
</databaseChangeLog>